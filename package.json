{
  "name": "dual-practitioner-frontend",
  "version": "0.1.0",
  "private": true,
  "dependencies": {
    "@reduxjs/toolkit": "^1.8.1",
    "@testing-library/jest-dom": "^5.16.4",
    "@testing-library/react": "^13.2.0",
    "@testing-library/user-event": "^13.5.0",
    "@types/jest": "^27.5.0",
    "@types/node": "^16.11.33",
    "@types/react": "^18.0.9",
    "@types/react-dom": "^18.0.3",
    "axios": "^0.27.2",
    "bootstrap": "^5.1.3",
    "env-cmd": "^10.1.0",
    "formik": "^2.2.9",
    "i18next": "^21.9.1",
    "react": "^18.1.0",
    "react-bootstrap": "^2.3.1",
    "react-dom": "^18.1.0",
    "react-i18next": "^11.18.4",
    "react-icons": "^4.3.1",
    "react-redux": "^8.0.1",
    "react-router-dom": "^6.3.0",
    "react-scripts": "5.0.1",
    "redux": "^4.2.0",
    "redux-saga": "^1.1.3",
    "sass": "^1.51.0",
    "typescript": "^4.6.4",
    "web-vitals": "^2.1.4",
    "yup": "^0.32.11"
  },
  "scripts": {
    "start": "react-scripts start",
    "build": "react-scripts build",
    "test": "react-scripts test",
    "test:coverage": "react-scripts test --collectCoverage --watchAll",
    "eject": "react-scripts eject",
    "stylelint": "stylelint \"**/*.scss\"",
    "eslint": "npx eslint ./src --max-warnings=2",
    "eslint:fix": "npx eslint ./src --fix",
    "lint": "npm run stylelint",
    "build:dev": "REACT_APP_ENV='dev' BUILD_PATH='./dist/development/' env-cmd -f ./src/environments/.env.development npm run build",
    "build:test": "REACT_APP_ENV='test' BUILD_PATH='./dist/testing/' env-cmd -f ./src/environments/.env.testing npm run build",
    "build:stg": "REACT_APP_ENV='stg' BUILD_PATH='./dist/staging/' env-cmd -f ./src/environments/.env.staging npm run build",
    "build:prod": "REACT_APP_ENV='prod' BUILD_PATH='./dist/production/' env-cmd -f ./src/environments/.env.production npm run build",
    "build-all": "npm-run-all --parallel build:*"
  },
  "eslintConfig": {
    "extends": [
      "react-app",
      "react-app/jest"
    ]
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "devDependencies": {
    "npm-run-all": "^4.1.5"
  }
}
